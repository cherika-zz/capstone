<h1><%= @state.name %></h1>

<div class="row">
  <div class="col-md-8">
    <!-- how many IDs does new_state_reg_path need? just one -->
    <%= link_to "New Regs", new_state_reg_path(@state), class: 'btn btn-success' %>
  </div>
</div>

<div class="row">
  <div class="col-md-8">
    <% @regs.each do |reg| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to reg.name, [@state, reg] %>
          </h4>
        </div>
      </div>
    <% end %>
  </div>
</div>
